            "dataType": "int64",
            "isHidden": true,
            "sourceColumn": "TicketID"
          },
          {
            "name": "OpenDateKey",
            "dataType": "int64",
            "sourceColumn": "OpenDateKey"
          },
          {
            "name": "CloseDateKey",
            "dataType": "int64",
            "sourceColumn": "CloseDateKey"
          },
          {
            "name": "CategoryKey",
            "dataType": "int64",
            "isHidden": true,
            "sourceColumn": "CategoryKey"
          },
          {
            "name": "SLAHours",
            "dataType": "int64",
            "sourceColumn": "SLAHours"
          },
          {
            "name": "ResolutionHours",
            "dataType": "int64",
            "sourceColumn": "ResolutionHours"
          },
          {
            "name": "Status",
            "dataType": "string",
            "sourceColumn": "Status"
          }
        ],
        "partitions": [
          {
            "name": "Partition",
            "dataView": "full",
            "source": {
              "type": "m",
              "expression": [
                "let",
                "    Source = #\"SQL/localhost;ITSM_Analytics\",",
                "    dbo_FactTickets = Source{[Schema=\"dbo\",Item=\"FactTickets\"]}[Data]",
                "in",
                "    dbo_FactTickets"
              ]
            }
          }
        ],
        "measures": [
          {
            "name": "Total Tickets",
            "expression": "COUNTROWS(FactTickets)"
          },
          {
            "name": "Avg Resolution Hours",
            "expression": [
              " AVERAGE(FactTickets[ResolutionHours])",
              ""
            ],
            "formatString": "0.0"
          },
          {
            "name": "SLA Breach Tickets",
            "expression": [
              " CALCULATE([Total Tickets], FactTickets[ResolutionHours] > FactTickets[SLAHours])",
              ""
            ],
            "formatString": "0"
          },
          {
            "name": "SLA Breach %",
            "expression": [
              " DIVIDE([SLA Breach Tickets], [Total Tickets])",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "kpi": {
              "targetExpression": "0.1",
              "targetFormatString": "0.00%;-0.00%;0.00%",
              "statusGraphic": "Traffic Light - Single",
              "statusExpression": [
                "var x='FactTickets'[SLA Breach %] return",
                "            if(ISBLANK(x),BLANK(),",
                "                    If(x<0.1,1,",
                "\t                If(x<0.2,0,-1)",
                "                )",
                "            )",
                "            "
              ],
              "annotations": [
                {
                  "name": "GoalType",
                  "value": "StaticValue"
                },
                {
                  "name": "KpiStatusType",
                  "value": "LinearReversed"
                },
                {
                  "name": "KpiThresholdType",
                  "value": "Absolute"
                },
                {
                  "name": "KpiThresholdOrdering",
                  "value": "Ascending"
                },
                {
                  "name": "KpiThresholdCount",
                  "value": "2"
                },
                {
                  "name": "KpiThreshold_0",
                  "value": "0.1"
                },
                {
                  "name": "KpiThreshold_1",
                  "value": "0.2"
                }
              ]
            }
          },
          {
            "name": "High Severity Tickets",
            "expression": [
              " CALCULATE([Total Tickets], FILTER(DimCategory, DimCategory[Severity] IN {1,2}))",
              ""
            ]
          },
          {
            "name": "High Severity Tickets %",
            "expression": [
              " DIVIDE([High Severity Tickets], [Total Tickets])",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "kpi": {
              "targetExpression": "0.3",
              "targetFormatString": "0.00%;-0.00%;0.00%",
              "statusGraphic": "Traffic Light - Single",
              "statusExpression": [
                "var x='FactTickets'[High Severity Tickets %] return",
                "            if(ISBLANK(x),BLANK(),",
                "                    If(x<0.3,1,",
                "\t                If(x<0.4,0,-1)",
                "                )",
                "            )",
                "            "
              ],
              "annotations": [
                {
                  "name": "GoalType",
                  "value": "StaticValue"
                },
                {
                  "name": "KpiStatusType",
                  "value": "LinearReversed"
                },
                {
                  "name": "KpiThresholdType",
                  "value": "Absolute"
                },
                {
                  "name": "KpiThresholdOrdering",
                  "value": "Ascending"
                },
                {
                  "name": "KpiThresholdCount",
                  "value": "2"
                },
                {
                  "name": "KpiThreshold_0",
                  "value": "0.3"
                },
                {
                  "name": "KpiThreshold_1",
                  "value": "0.4"
                }
              ]
            }
          }
        ]
