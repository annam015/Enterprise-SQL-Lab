  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=DateAdd("d", -7, Globals!ExecutionTime)</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Globals!ExecutionTime</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  HourBucket,
  OrdersCount,
  TotalAmount,
  AvgAmount
FROM dbo.FactOrders_ByHour
WHERE HourBucket &gt;= @StartDate
  AND HourBucket &lt;  @EndDate
ORDER BY HourBucket;</CommandText>
      </Query>
      <Fields>
        <Field Name="HourBucket">
          <DataField>HourBucket</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="OrdersCount">
          <DataField>OrdersCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalAmount">
          <DataField>TotalAmount</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="AvgAmount">
          <DataField>AvgAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
